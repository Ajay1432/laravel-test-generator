{
    "name": "ajay-makwana/laravel-test-generator",
    "description": "Laravel package for generating unit test automatically",
    "license": "MIT",
    "authors": [
        {
            "name": "Vignesh C",
            "email": "vignesh.jag@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "fzaninotto/faker": "^1.8",
        "laravel/framework": "^8.18",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "AjayMakwana\\LaravelTestGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AjayMakwana\\LaravelTestGenerator\\TestGeneratorServiceProvider"
            ]
        }
    }
}
